#include <stdio.h>

struct teams
{
    char a[100];
};

struct teams cur_team[100];

int main(void) {
    FILE *myfile = fopen("Members.txt", "r");
    char ch;
    int count = 0;

        do{
		ch = fgetc(myfile);
        if (ch == '\n') count++;
        }
        while (ch != EOF);

    char *A,*B,*C,*D,*E,*F,*G,*H,*I,*J;

	rewind(myfile);
        int i;
        for (i = 0; i <= count; i++) {

        fscanf(myfile, "%s", cur_team[i].a);

        }
        A=&cur_team[0].a;
        B=&cur_team[1].a;
        C=&cur_team[2].a;
        D=&cur_team[3].a;
        E=&cur_team[4].a;
        F=&cur_team[5].a;
        G=&cur_team[6].a;
        H=&cur_team[7].a;
        I=&cur_team[8].a;
        J=&cur_team[9].a;


return 0;
    }

